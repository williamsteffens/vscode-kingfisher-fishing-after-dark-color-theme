	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d24b4b"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		// KEYWORDS ----------------------------------------------------------------------------------------------------
		{
			"name": "Keyword, Storage, Control Flow, Terminator",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"punctuation.terminator",
				"keyword.operator.new"
			],
			"settings": {
				"fontStyle": "bold",
				// steel blue 64a2d4
				// green color: 017b84
				// 018f99
				"foreground": "#017b84"
			}
		},
		{
			"name": "Variables, Operator, Misc, Other",
			"scope": [
				"variable",
				"variable.other",
				"string constant.other.placeholder",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.operator",
				"keyword.other.template",
				"keyword.other.substitution",
				"meta"
			],
			"settings": {
				// E1E4E8
				// til noget andet 627a9e måske parameter
				// e3d5b8
				// func: e6cb92
				// decfac
				"foreground": "#d8d3c6"
			}
		},
		// PUNCTUATION -------------------------------------------------------------------------------------------------
		{
			"name": "Array Punctuation",
			"scope": [
				"meta.array.literal",
				"punctuation.separator",
			],
			"settings": {
				"foreground": "#017b84"
			}
		},
		// VALUES ------------------------------------------------------------------------------------------------------
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
			],
			"settings": {
				// steelblue : 64a2d4
				// D4910C
				// d2910e
				// efa51c
				// e09b1a
				// d48d08
				// dac17e
				// F89880
				// d1a42f
				// de7b2f

				// f39b16
				// f09000

				// for the belted version: d96900
				// ffaf69
				// f2ad70
				// f39644


				// d37487

				// c56391
				// cc7ba1
				"foreground": "#d37487"
			}
		},
		{
			"name": "Support Variables",
			"scope": [
				"support.variable"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#64a2d4"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ff00bf"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				// try the teal one, otherwise switch back to 64af52
				// 22d6c1
				// 25ee7f
				// 00e2a9
				"foreground": "#22d6c1"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#f99188"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				// F88379
				"foreground": "#ff897e"
			}
		},
		// FUNCTION ----------------------------------------------------------------------------------------------------
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",

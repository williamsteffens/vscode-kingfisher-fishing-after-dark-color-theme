	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e04242"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#e04242"
			}
		},

		// KEYWORDS ----------------------------------------------------------------------------------------------------
		{
			"name": "Keyword, Storage, Control Flow, Terminator, Language Methods",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"punctuation.terminator",
				"punctuation.separator",
				"keyword.operator.new",
				"variable.language",
				"support.type",
				"keyword.operator.c",
			],
			"settings": {
				"fontStyle": "bold",

				// 9ba5a4
				// 7e8c8a
				// 67938c
				// 5ec9b9

				// 248377
				// 3d8379
				// 4e867d

				// 2e8779


				// til noget andet 
				// 206b80
				// 0d6e89

				// 228877
				// 017371
				// 07807d
				"foreground": "#07807d"
			}
		},
		{
			"name": "Variables, Operator, Misc, Other",
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
				"string constant.other.placeholder",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.operator",
				"keyword.other.template",
				"keyword.other.substitution",
				"text",
				"punctuation.separator.key-value.html",
				"meta"
			],
			"settings": {
				"foreground": "#d3d0c3"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#d3d0c3"
			}
		},

		// VALUES ------------------------------------------------------------------------------------------------------
		{
			"name": "Number, Embedded",
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"support.constant",
				
			],
			"settings": {
				// 7cb9eb

				// 7ea59e
				//64a2d4

				"foreground": "#64a2d4"
			}
		},
		{
			"name": "Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"constant.character",
				"constant.escape",
			],
			"settings": {
				"foreground": "#f2a100"
			}
		},
		{
			"name": "Other Variable, String Link",
